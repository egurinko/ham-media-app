{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "codegen": "yarn codegen:internal && yarn codegen:public",
    "codegen:internal": "yarn apollo codegen:generate --localSchemaFile='../graphql/internalApi/schema.graphql' --target=typescript --tagName=gql --includes='./api/internal_api/**.ts'",
    "codegen:public": "yarn apollo codegen:generate --localSchemaFile='../graphql/publicApi/schema.graphql' --target=typescript --tagName=gql --includes='./api/public_api/**.ts'"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "js-cookie": "^2.2.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.7",
    "@types/react": "17.0.14",
    "apollo": "^2.33.4",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
}
