{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "console": "yarn --cwd ../ prisma-repl",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NO_PEER_DEPENDENCY_CHECK=true node dist/server",
    "dev": "ts-node-dev -r tsconfig-paths/register server --transpile-only",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:reflect": "prisma generate",
    "db:seed:prd": "NODE_ENV=production prisma db seed",
    "db:seed:dev": "NODE_ENV=development prisma db seed",
    "tsc": "tsc"
  },
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@line/bot-sdk": "^7.4.0",
    "@prisma/client": "^3.5.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "fastify": "^3.24.0",
    "fastify-cli": "^2.13.0",
    "fastify-cors": "^6.0.2",
    "fastify-cron": "^1.1.2",
    "fastify-helmet": "^5.3.2",
    "fastify-plugin": "^3.0.0",
    "graphql-relay": "^0.9.0",
    "json-bigint-patch": "^0.0.4",
    "jsonwebtoken": "^8.5.1",
    "mercurius": "^8.9.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "pino-pretty": "^6.0.0",
    "prisma": "^3.5.0",
    "prisma-repl": "^1.3.0",
    "sanitize-html": "^2.5.3",
    "ts-node-dev": "^1.1.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-relay": "^0.7.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.9",
    "@types/sanitize-html": "^2.5.0",
    "@types/uuid": "^8.3.3",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.12.0"
  }
}
