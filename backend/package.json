{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "yarn": ">=1.22.17 <2.0.0"
  },
  "scripts": {
    "console": "yarn --cwd ../ prisma-repl",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NO_PEER_DEPENDENCY_CHECK=true node dist/server",
    "dev": "ts-node-dev -r tsconfig-paths/register server --transpile-only",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:push:test": "dotenv -e .env.test prisma db push",
    "db:reflect": "prisma generate",
    "db:seed:prd": "NODE_ENV=production prisma db seed",
    "db:seed:dev": "NODE_ENV=development prisma db seed",
    "db:seed:test": "NODE_ENV=test dotenv -e .env.test prisma db seed",
    "tsc": "tsc",
    "tscheck": "yarn tsc -p ./tsconfig.json --noEmit",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest --runInBand",
    "test:ci": "NODE_ENV=test dotenv -e .env.test -- jest --runInBand --coverage --silent --testLocationInResults --ci --json --outputFile=\"report.json\""
  },
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/lib-storage": "^3.470.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/helmet": "^11.1.1",
    "@line/bot-sdk": "^8.0.2",
    "@prisma/client": "5.7.0",
    "@sentry/node": "^7.84.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "fastify": "^4.24.3",
    "fastify-cli": "^5.9.0",
    "fastify-healthcheck": "^4.4.0",
    "fastify-plugin": "^4.5.1",
    "graphql-relay": "^0.10.0",
    "json-bigint-patch": "^0.0.8",
    "jsonwebtoken": "^9.0.2",
    "mercurius": "^13.3.1",
    "mercurius-upload": "^7.0.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.1",
    "node-cron": "^3.0.3",
    "pino-pretty": "^10.2.3",
    "prisma": "5.7.0",
    "prisma-repl": "^1.4.0",
    "sanitize-html": "^2.11.0",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/graphql-relay": "^0.7.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.18.8",
    "@types/node-cron": "^3.0.11",
    "@types/sanitize-html": "^2.9.5",
    "@types/uuid": "^9.0.7",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "mercurius-integration-testing": "^8.1.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  }
}
