{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "console": "yarn --cwd ../ prisma-repl",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NO_PEER_DEPENDENCY_CHECK=true node dist/server",
    "dev": "ts-node-dev -r tsconfig-paths/register server --transpile-only",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:push:test": "dotenv -e .env.test prisma db push",
    "db:reflect": "prisma generate",
    "db:seed:prd": "NODE_ENV=production prisma db seed",
    "db:seed:dev": "NODE_ENV=development prisma db seed",
    "db:seed:test": "NODE_ENV=test dotenv -e .env.test prisma db seed",
    "tsc": "tsc",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest --runInBand"
  },
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.0",
    "@aws-sdk/lib-storage": "^3.54.0",
    "@line/bot-sdk": "^7.4.0",
    "@prisma/client": "^3.9.0",
    "@sentry/node": "^6.18.2",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "fastify": "^3.27.4",
    "fastify-cli": "^2.15.0",
    "fastify-cors": "^6.0.3",
    "fastify-cron": "^1.3.0",
    "fastify-helmet": "^7.0.1",
    "fastify-plugin": "^3.0.1",
    "graphql-relay": "^0.10.0",
    "json-bigint-patch": "^0.0.4",
    "jsonwebtoken": "^8.5.1",
    "mercurius": "^8.9.1",
    "mercurius-upload": "^3.0.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "pino-pretty": "^6.0.0",
    "prisma": "^3.9.0",
    "prisma-repl": "^1.3.0",
    "sanitize-html": "^2.7.0",
    "ts-node-dev": "^1.1.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-relay": "^0.7.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/sanitize-html": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "mercurius-integration-testing": "^4.0.0",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.13.0"
  }
}
